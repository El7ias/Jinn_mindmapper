# Environment Variables Template
# Copy to .env and fill in your values

# Firebase Configuration (for Vite client)
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_ID=000000000000
VITE_FIREBASE_APP_ID=1:000000000000:web:0000000000000000

# Cloud Functions base URL
VITE_FUNCTIONS_BASE_URL=https://us-central1-your-project.cloudfunctions.net

# Use Firebase emulators in development (set to 'true' to enable)
VITE_USE_EMULATORS=false
